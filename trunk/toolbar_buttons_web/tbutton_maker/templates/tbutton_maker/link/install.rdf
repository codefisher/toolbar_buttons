<Description about="urn:mozilla:install-manifest">
<em:id>{{ extension_uuid }}</em:id>
<em:name>{{ name }}</em:name>
<em:version>1.1.0</em:version>
<em:description>A Customized Link Toolbar Button.</em:description>
<em:creator>Michael Buckley</em:creator>
<em:iconURL>chrome://{{ chrome_name }}/skin/icon.png</em:iconURL>
<em:homepageURL>{{ home_page }}</em:homepageURL>
<em:updateURL>{{ update_url }}</em:updateURL>
<em:optionsURL>chrome://lbutton/content/option_window.xul</em:optionsURL>

<!-- Firefox -->
<em:targetApplication>
 <Description>
    <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
    <em:minVersion>3.0</em:minVersion>
    <em:maxVersion>{{ max_version }}</em:maxVersion>
 </Description>
</em:targetApplication>

</Description>
</RDF>
